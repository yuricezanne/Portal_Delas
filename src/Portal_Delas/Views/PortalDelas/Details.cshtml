@using Core.Models;
@using Portal_Delas.Models;
@using Core.Data;
@model Views_PortalDelas_Create

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{

	ViewData["Title"] = "Details";

}


<h2>Details</h2>
<div class="row">
	<div class="col-md-4">
		<dl>
			<dt>
				Id
			</dt>
			<dd>
				@Model.JobId
			</dd>
			<dt>
				Date
			</dt>
			<dd>
				@Model.JobCreationDate
			</dd>
			<dt>
				Title
			</dt>
			<dd>
				@Model.JobTitle
			</dd>
			<div class="form-group">
				<a asp-action="Edit" asp-route-id="@Model.JobId" class="btn btn-primary">Edit</a>
				<a asp-action="Index" class="btn btn-secondary">Back to List</a>
			</div>
		</dl>
	</div>

</div>

@section Scripts {
	@{
		await Html.RenderPartialAsync("_ValidationScriptsPartial");
	}
}


